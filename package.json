{
  "name": "vue-ts-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build --mode development && node toUnCross.mjs",
    "build:demo": "vue-tsc --noEmit && vite build --mode production && node toFile.mjs",
    "build:test": "vue-tsc && vite build --mode test",
    "build:prod": "vue-tsc && vite build --mode prod",
    "preview": "vite preview",
    "eslint": "eslint src",
    "eslint-fix": "eslint --fix src",
    "format": "prettier --write \"./**/*.{ts,tsx,js,json,vue,html,md}\"",
    "lint:eslint": "eslint src/**/*.{ts,vue} --cache --fix",
    "lint:style": "stylelint src/**/*.{less,scss,md,vue,,json,ts,tsx} --cache --fix",
    "prepare": "husky install",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "preinstall": "node ./scripts/preinstall.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@types/swiper": "^6.0.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "element-plus": "^2.3.5",
    "fs-extra": "^11.3.0",
    "gsap": "^3.12.2",
    "lenis": "^1.1.20",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.3",
    "qs": "^6.13.0",
    "simplex-noise": "^4.0.1",
    "swiper": "^10.2.0",
    "unocss": "^65.4.3",
    "vue": "^3.5.13",
    "vue-router": "^4.2.1",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/eslint-parser": "^7.21.8",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@esbuild/darwin-x64": "0.19.12",
    "@types/crypto-js": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-vue": "^9.13.0",
    "husky": "^8.0.0",
    "imagemin-cli": "^7.0.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "stylelint": "^15.6.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-scss": "^11.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "terser": "^5.19.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-mock": "2.9.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^1.4.2"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.19.12"
    }
  }
}
